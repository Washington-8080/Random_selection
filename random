def find_pair(arr, target):
    for i in range(len(arr)): 

        for j in range(i + 1, len(arr)):  

            if arr[i] + arr[j] == target:
                return arr[i], arr[j] 

    return None 

arr = [3, 1, 5, 2, 4]
target = 9
result = find_pair(arr, target)

print(result if result else "No pair found")
